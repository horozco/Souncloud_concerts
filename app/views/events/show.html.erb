<fieldset>
  <legend>Event Content</legend>

Posted By: <%=@event.user.full_name %> | 
  <% if current_user == @event.user%>
    <%= link_to 'Edit Event', edit_event_path(@event), class: "btn-link" %>
  <%end%>
  <div class = "well">
    <p><b>Date: </b> <%= @event.date%></p>
    <b>Artist Name: </b> <%= @event.artist_name%></p>
    <b>Time And Duration: </b> <%= @event.time%></p>
    <b>Created By: </b> <%= @event.user.full_name%></p>
  </div>

</fieldset>